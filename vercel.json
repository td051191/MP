{ "version": 2, "builds": [ { "src": "client/**/*", "use": "@vercel/static-build", "config": { "distDir": "dist/spa" } }, { "src": "server/index.ts", "use": "@vercel/node" } ], "routes": [ { "src": "/api/(.*)", "dest": "/server/index.ts" }, { "src": "/(.*)", "dest": "/dist/spa/$1" } ], "env": { "DATABASE_URL": "@database_url" } }